<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.ikahimki.ikahimki.MenuAwal">

<ImageButton
    android:contentDescription="@string/tolmen"
    android:layout_width="100dp"
    android:layout_height="100dp"
    android:src="@drawable/mulaiicon2"
    android:id="@+id/tolmen"
    android:onClick="MasukMenuBtn1"
    android:layout_marginBottom="121dp"
    android:background="#0000"
    android:layout_alignParentBottom="true"
    android:layout_centerHorizontal="true" />
<ImageView
    android:contentDescription="@string/ikafont"
    android:layout_width="350dp"
    android:layout_height="100dp"
    android:src="@drawable/logoikahimkifont2"
    android:layout_above="@+id/tolmen"
    android:layout_centerHorizontal="true"
    android:layout_marginBottom="91dp"
    android:id="@+id/imageView" />
</RelativeLayout>
